{
  "name": "vue_3_starter_plugin",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "watch": "webpack --watch --config webpack.config.dev.js",
    "uxp:load": "cd dist && uxp plugin load",
    "uxp:reload": "cd dist && uxp plugin reload",
    "uxp:watch": "cd dist && nodemon --exec \"uxp plugin reload\" -e js,css,html",
    "uxp:debug": "cd dist && uxp plugin debug"
  },
  "license": "none",
  "private": true,
  "devDependencies": {
    "@vue/compiler-sfc": "^3.2.22",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^5.2.7",
    "file-loader": "^5.1.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.7",
    "pinia": "^2.0.33",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "style-loader": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "vue-loader": "^16.8.3",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "vue": "^3.2.22"
  },
  "resolutions": {
    "acorn": "npm:acorn-with-stage3"
  }
}
